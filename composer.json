{
    "name": "yamilovs/sms-bundle",
    "type": "symfony-bundle",
    "description": "Sms sending system with different providers",
    "keywords": [
        "Sms", "sms.ru", "smsaero.ru", "iqsms.ru", "smsc.ru", "messagebird.com"
    ],
    "license": "MIT",
    "homepage": "https://github.com/yamilovs/SmsBundle",
    "authors": [
        {
            "name": "Yamilov Stepan",
            "email": "yamilovs@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/framework-bundle": "^3.4|^4.4",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "symfony/console": "^4.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Yamilovs\\Bundle\\SmsBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yamilovs\\Bundle\\SmsBundle\\Tests\\": "tests/"
        }
    }
}
